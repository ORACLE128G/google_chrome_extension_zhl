{

  "name": "zhl_extension",

  "description": "this is zhl's extension",

  "version": "1.2",

  "manifest_version": 2,

  "icons": {
        "128": "img/cloud_128.png",
        "16": "img/cloud_16.png",
        "48": "img/cloud_48.png"
    },

  "chrome_url_overrides": {

    "newtab": "zhl.html"

  } ,
  "permissions": [ "notifications", "unlimitedStorage", "downloads", "storage", "contextMenus", "tabs",  "webRequest", "webRequestBlocking"],
  "web_accessible_resources": [ "img/*"  , "js/*" , "video/*"] 
}